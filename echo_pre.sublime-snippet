    <content>
        <![CDATA[echo '<pre>'; print_r(${1}); echo '<pre>'; ]]>
    </content>
    <tabTrigger>epre</tabTrigger>
    <scope>source.php</scope>
    <description>echo pre</description>
</snippet>
